{
    "name": "apiconfiguration",
    "version": "1.0.0",
    "private": true,
    "description": "HighQ",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "devDependencies": {
        "@sap-ux/ui5-middleware-fe-mockserver": "^2.1.47",
        "@sap/ux-specification": "UI5-1.71",
        "@sap/ux-ui5-tooling": "^1.8.4",
        "@ui5/cli": "^2.14.17",
        "rimraf": "^3.0.2",
        "ui5-task-flatten-library": "0.2.5"
    },
    "scripts": {
        "start": "fiori run --open \"test/flpSandbox.html?sap-client=200&sap-ui-xx-viewCache=false#comresultoconsultoriatesthighqapiconfiguration-tile\"",
        "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-client=200&sap-ui-xx-viewCache=false#comresultoconsultoriatesthighqapiconfiguration-tile\"",
        "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "deploy-config": "fiori add deploy-config",
        "start-noflp": "fiori run --open \"index.html?sap-client=200&sap-ui-xx-viewCache=false\"",
        "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-client=200&sap-ui-xx-viewCache=false#comresultoconsultoriatesthighqapiconfiguration-tile\"",
        "start-variants-management": "fiori run --open \"preview.html?&sap-client=200&sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml",
        "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "@sap-ux/ui5-middleware-fe-mockserver",
            "ui5-task-flatten-library"
        ]
    },
    "sapux": true,
    "sapuxLayer": "CUSTOMER_BASE"
}
